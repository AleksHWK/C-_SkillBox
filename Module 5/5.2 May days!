#include <iostream>
using namespace std;

int main() {
    setlocale(LC_ALL, "rus");
    cout << "-----Выходные в мае!-----" << endl << endl;

    unsigned short int day;

    cout << "Введите дату: ";
    cin >> day;

    if ((day >= 1 and day <= 10) or (day >= 13 and day <= 14) or (day >= 21 and day <= 22) or (day >= 27 and day <= 28)) {
        cout << "Выходной!";
    }
    else if (day >= 32) {
        cout << "Уже не май)";
    }
    else {
        cout << "Рабочий день!";
    }



    return 0;
}
