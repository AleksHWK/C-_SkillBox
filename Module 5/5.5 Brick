#include <iostream>
using namespace std;

int main() {
    setlocale(LC_ALL, "rus");
    cout << "-----Кирпичи!-----" << endl;

    unsigned int A, B, C, M, N, K;
    cout << "Введите сторону кирпичика 1 (А): ";
    cin >> A;
    cout << "Введите сторону кирпичика 1 (B): ";
    cin >> B;
    cout << "Введите сторону кирпичика 1 (C): ";
    cin >> C;
    cout << "Введите сторону кирпичика 2 (M): ";
    cin >> M;
    cout << "Введите сторону кирпичика 2 (N): ";
    cin >> N;
    cout << "Введите сторону кирпичика 2 (K): ";
    cin >> K;
    
    int min1 = min(min(A, B), C);
    int max1 = max(max(A, B), C);
    int mid1 = A + B + C - min1 - max1;

    int min2 = min(min(M, N), K);
    int max2 = max(max(M, N), K);
    int mid2 = M + N + K - min2 - max2;

    if (min1 < min2 && mid1 < mid2 && max1 < max2) {
        cout << "Можно поместить первую коробку во вторую!";
    }
    else {
        cout << "Нельзя поместить первую коробку во вторую.";
    }
   
    return 0;
}
