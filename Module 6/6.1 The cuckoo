#include <iostream>

using namespace std;


int main() {
    setlocale(LC_ALL, "rus");
    cout << "-----Кукушка------" << endl;

    int temp;
    cout << "Введите время: ";
    cin >> temp;
    if (temp < 0 or temp > 12) {
        cout << "Ошибка.";
        return 1;
    }
        while (temp--) {
            cout << "Ку Ку" << endl;
        }

    return 0;
}
