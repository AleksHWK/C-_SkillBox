#include <iostream>

int main() {
	setlocale(LC_ALL, "Russian");
	std::cout << "Минимум из двух чисел.\n\n";

	int a;
	std::cout << "Введите первое число: ";
	std::cin >> a;

	int b;
	std::cout << "Введите второе число: ";
	std::cin >> b;

	if (a > b) {
		std::cout << "Наименьшее число: " << b;
	}
	else if (a < b) {
		std::cout << "Наименьшее число: " << a;
	}else 
		std::cout << "Числа равны! ";
}
