#include <iostream>
#include <cmath>

using namespace std;


int main() {
    setlocale(LC_ALL, "rus");
    cout << "-----Кукушка------" << endl;
    
    int temp;
    cout << "Введите время: ";
    cin >> temp;
    if (temp < 0 or temp > 12) {
        cout << "Ошибка.";
        return 0;
    }
    else {
        int i = 0;
        while (temp > i) {
            cout << "Ку Ку" << endl;
            i++;
        }
    }

    return 0;
}
